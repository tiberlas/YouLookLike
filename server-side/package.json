{
	"name": "you-look-like",
	"version": "0.0.1",
	"description": "It's a web (in future a mobile) application for finding what other people thing about you.",
	"main": "index.js",
	"scripts": {
		"start": "node dist/index.js",
		"dev": "nodemon src/index.ts",
		"build": "tsc -p .",
		"dev-tools": "node --inspect --inspect-brk src/index.js",
		"test": "mocha test.ts --require ts-node/register test/*.test.ts --recursive || true",
		"test-watch": "mocha test.ts --require ts-node/register test/**/*.test.ts --recursive  -w",
		"test-coverage": "nyc npm test",
		"test-one": "mocha test.ts --require ts-node/register test/error/*.test.ts"
	},
	"author": "tiberius",
	"license": "MIT",
	"dependencies": {
		"express": "^4.17.1",
		"jsonwebtoken": "^8.5.1",
		"mongodb": "^3.6.0",
		"uuid": "^8.3.1"
	},
	"devDependencies": {
		"@types/chai": "^4.2.12",
		"@types/express": "^4.17.7",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/mocha": "^8.0.3",
		"@types/moment": "^2.13.0",
		"@types/mongoose": "^5.7.34",
		"@types/node": "^14.0.27",
		"@types/sinon": "^9.0.8",
		"@types/supertest": "^2.0.10",
		"@types/uuid": "^8.0.0",
		"chai": "^4.2.0",
		"mocha": "^8.1.1",
		"nodemon": "^2.0.4",
		"nyc": "^15.1.0",
		"sinon": "^9.2.0",
		"supertest": "^4.0.2",
		"ts-node": "^8.10.2",
		"typescript": "^3.9.7"
	}
}
